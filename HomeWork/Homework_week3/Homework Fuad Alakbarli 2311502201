public class CarpimTablosu {
    public static void main(String[] args) {
        int boyut = 10; 
        
        // Çarpım tablosunu oluşturma
        for (int i = 1; i <= boyut; i++) {
            for (int j = 1; j <= boyut; j++) {
                System.out.printf("%4d", i * j); 
            }
            System.out.println();
